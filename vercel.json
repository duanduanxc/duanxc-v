{
  "rewrites": [
    { "source": "/https/:match/*", "destination": "https://:match/*" },
    { "source": "/http/:match/*", "destination": "http://:match/*" },
    { "source": "/wss/:match/*", "destination": "wss://:match/*" },
    { "source": "/:protocol/:match/*", "destination": ":protocol://:match/*" }
  ]
}

